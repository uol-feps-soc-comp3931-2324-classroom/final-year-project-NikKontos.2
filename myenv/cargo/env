#!/bin/sh
# rustup shell setup
# affix colons on either side of $PATH to simplify matching
case ":${PATH}:" in
    *:"/Users/Nik/July_Git_Repo/final-year-project-NikKontos.2/myenv/cargo/bin":*)
        ;;
    *)
        # Prepending path in case a system-installed rustc needs to be overridden
        export PATH="/Users/Nik/July_Git_Repo/final-year-project-NikKontos.2/myenv/cargo/bin:$PATH"
        ;;
esac
